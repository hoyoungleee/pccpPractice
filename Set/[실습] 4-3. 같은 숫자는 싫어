import java.util.*;

public class Solution {
    public int[] solution(int []arr) {
        int[] answer = {};
        
        //중복제거용 셋
        Set<Integer> set = new HashSet<>();
        
        //임시 저장 리스트
        List<Integer> list = new ArrayList<>();
        
        int prev = arr[0];
        for(int num : arr){
            if(!set.add(num)){
                if(prev == num){
                    continue;
                }else{
                    list.add(num);
                }
            }else{
                list.add(num);
            }
            prev = num;
        }
        
        answer = list.stream().mapToInt(i -> i).toArray();

        return answer;
    }
}