class Solution {
    boolean solution(String s) {
        boolean answer = true;
        String str = s.toLowerCase();
        int pCnt = 0;
        int yCnt = 0;
        for(char c : str.toCharArray()){
            if(c == 'p'){
                pCnt += 1;
            }else if(c == 'y'){
                yCnt += 1;
            }
        }
        if(pCnt != yCnt){
            return false;
        }

        return answer;
    }
}