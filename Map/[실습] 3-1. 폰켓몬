import java.util.*;

class Solution {
    public int solution(int[] nums) {
        int n = nums.length /2;
        Map<Integer, Integer> map = new HashMap<>();
        
        for(int i = 0; i < nums.length; i++){
            map.put(nums[i], nums[i]);
        }
        int spieces = map.keySet().size();
        
        int answer = Math.min(n, spieces);
        
        
        
        return answer;
    }
}